# Agency Uptime Frontend Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Backend API URL
# Point to your backend server
NEXT_PUBLIC_API_URL=http://localhost:5000

# Stripe Configuration (Public Keys Only)
# Get your publishable key from Stripe Dashboard: https://dashboard.stripe.com/apikeys
# TEST KEY (for development)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# PRODUCTION KEY (for live environment)
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_live_stripe_publishable_key_here

# Application Configuration
NEXT_PUBLIC_APP_NAME=Agency Uptime
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags (Optional)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=true

# Optional: Analytics & Monitoring
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_HOTJAR_ID=your-hotjar-id
# NEXT_PUBLIC_SENTRY_DSN=your-sentry-frontend-dsn

# Optional: Third-party Integrations
# NEXT_PUBLIC_CRISP_WEBSITE_ID=your-crisp-chat-id
# NEXT_PUBLIC_INTERCOM_APP_ID=your-intercom-app-id 